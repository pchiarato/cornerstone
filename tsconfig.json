{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "declaration": true,
        "pretty": true,
        "outDir": "build"
    },
    "exclude": [
        "node_modules",
        "build",
        "src/unused"
    ]
}