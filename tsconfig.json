{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "declaration": true,
        "pretty": true,
        "outDir": "./build"
    },
    "exclude": [
        "node_modules",
        "dist",
        "build"
    ]
}