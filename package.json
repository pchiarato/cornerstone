{
  "name": "angular2_cornerstone",
  "version": "0.0.1a",
  "description": "Port of cornerstone to angular2",
  "keywords": [
    "DICOM",
    "medical",
    "imaging"
  ],
  "author": "Ghet`",
  "homepage": "http://ghetolay.github.io/cornerstone/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghetolay/cornerstone.git"
  },
  "scripts": {
    "postinstall": "typings install",
    "lint": "tslint src/*.ts src/**/*.ts examples/*.ts examples/**/*.ts",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "prepublish": "tsc && tsc --p tsconfig.bundle.json && uglifyjs2 dist/ng-cornerstone.js -o dist/ng-cornerstone.min.js"
  },
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.23",
    "html-webpack-plugin": "^2.19.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "uglify-js2": "^2.1.11",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
